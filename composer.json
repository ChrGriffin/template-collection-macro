{
    "name": "chrgriffin/<PACKAGE_NAME>",
    "description": "Laravel Collection macro: DESCRIPTION HERE.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Griffin",
            "email": "christian.griffin@live.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "<PACKAGE_NAMESPACE>\\": "src",
            "<PACKAGE_NAMESPACE>\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "<PACKAGE_NAMESPACE>\\ServiceProvider"
            ]
        }
    }
}
